generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id          Int           @id @default(autoincrement())
  first_name  String
  last_name   String
  email       String        @unique
  password    String
  roles       String[]
  test_access test_access[]
}

model test_access {
  id      Int   @id @default(autoincrement())
  test_id Int
  user_id Int
  users   users @relation(fields: [user_id], references: [id])
}
